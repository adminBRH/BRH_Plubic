    <script>
        function alertModalWarning(modalID, Text) {
            var lbl = document.getElementById('<%= lbl_ModalWarning_Text.ClientID %>');
            lbl.innerText = Text;
            modalID = '#Modal_Warning';
            alertModal(modalID);
        }
        function alertModalInfo(modalID, Text) {
            var lbl = document.getElementById('<%= lbl_ModalInfo_Text.ClientID %>');
            lbl.innerText = Text;
            modalID = '#Modal_Info';
            alertModal(modalID);
        }
        function alertModal(modalID) {
            $(window).load(function () {
                $(modalID).modal('show');
            });
        }
    </script>

