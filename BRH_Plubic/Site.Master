				</div>
				<!-- Login Template -->
			</div>
		</div>
	</div>
    <!-- Modal Login ----------------------------------------------------------------------------- -->

    <!-- Modal Logout ----------------------------------------------------------------------------- -->
        <div class="modal fade" id="ModalLogout" tabindex="-1" role="dialog" aria-labelledby="ModalLogoutTitle" aria-hidden="true">
          <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="ModalLogoutTitle">You want to log out.</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body mx-auto text-center">
                <button type="button" class="btn btn-danger mx-5" data-dismiss="modal">No</button>
                <button type="button" id="btn_logout" class="btn btn-success mx-5" onserverclick="btn_logout_ServerClick" runat="server">Yes</button>
              </div>
              <div class="modal-footer">
                
              </div>
            </div>
          </div>
        </div>
    <!-- Modal Logout ----------------------------------------------------------------------------- -->

    <!-- Modal forgot password ----------------------------------------------------------------------------- -->
        <div class="modal fade" id="ModalforgotPass" tabindex="-1" role="dialog" aria-labelledby="ModalforgotPassTitle" aria-hidden="true">
          <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="ModalforgotPassTitle">Forgot password.</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body mx-auto text-center">
                  <div class="row col-12 mx-auto">
                      <div class="col-4 mx-auto text-center">
                        <label class="switch">
                            <input type="checkbox" id="CB_DC" name="CB_FG_Logintype" value="Doctor" checked onclick="CheckUserType('Doctor')" runat="server" />
                            <span class="slider round"></span>
                        </label><br />Doctor
                    </div>
                    <div class="col-4 mx-auto text-center">
                        <label class="switch">
                            <input type="checkbox" id="DB_OS" name="CB_FG_Logintype" value="outsource" onclick="CheckUserType('OutSource')" runat="server" />
                            <span class="slider round"></span>
                        </label><br />OutSource
                    </div>
                    <div class="col-4 mx-auto text-center">
                        <label class="switch">
                            <input type="checkbox" id="CB_EM" name="CB_FG_Logintype" value="Employee" onclick="CheckUserType('Employee')" runat="server" />
                            <span class="slider round"></span>
                        </label><br />Employee
                    </div>
                  </div>
                  <input type="text" id="txt_forgotpass" class="form-control" value="" placeholder="ระบุ User ID" runat="server" style="font-size: x-large;" />
                  ระบบจะทำการส่งรหัสยืนยันการ reset password ให้ท่านผ่านทาง email<br />
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-danger mx-5" data-dismiss="modal">Close</button>
                <button type="button" id="btn_forgotpass" class="btn btn-success mx-5" onserverclick="btn_forgotpass_ServerClick" runat="server">Send</button>
              </div>
            </div>
          </div>
        </div>
    <!-- Modal forgot password ----------------------------------------------------------------------------- -->
<script>
    function CheckUserType(S) {
        var C = document.getElementById('<%= CB_DC.ClientID %>');
        var E = document.getElementById('<%= CB_EM.ClientID %>');
        var O = document.getElementById('<%= DB_OS.ClientID %>');
        var txtHLoginType = document.getElementById('<%= txtH_LoginType.ClientID %>');

        if (S == 'Employee') {
            C.checked = false;
            E.checked = true;
            O.checked = false;
        }
        else if (S == 'OutSource') {
            C.checked = false;
            E.checked = false;
            O.checked = true;
        }
        else {
            C.checked = true;
            E.checked = false;
            O.checked = false;
        }

        txtHLoginType.value = S;
    }
</script>


