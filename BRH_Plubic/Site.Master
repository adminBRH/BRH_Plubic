    <script>
        function LoopStar(max, score, size) {
            var html = '';
            if (parseFloat(score) <= max) {
                var half = 'no';
                if ((score % 1) > 0) {
                    half = 'yes';
                }
                size = "ti-" + size + "x";
                for (var i = 0; i < parseInt(max); i++) {
                    if (i < parseInt(score)) {
                        html = html + '<span class="ti-star star on ' + size + '"></span> ';
                    }
                    else {
                        if (half == 'yes') {
                            html = html + '<span class="ti-star star half ' + size + '"></span> ';
                            half = 'no';
                        }
                        else {
                            html = html + '<span class="ti-star star ' + size + '"></span> ';
                        }
                    }
                }
            }
            //html = html + "<br />" + score;
            document.write(html);
        }

        function Today(intervalDay) {
            var today = new Date();
            var dd = String(today.getDate() + parseInt(intervalDay)).padStart(2, '0');
            var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
            var yyyy = today.getFullYear();
            today = yyyy + '-' + mm + '-' + dd;
            return today;
        }

        function DayOfWeek() {
            var days = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'];
            var d = new Date();
            var dayName = days[d.getDay()];
            document.write(dayName);
        }

        function monthTH(num) {
            var month = ['ม.ค.', 'ก.พ.', 'มี.ค.', 'เม.ย.', 'พ.ค.', 'มิ.ย.', 'ก.ค.', 'ส.ค.', 'ก.ย.', 'ต.ค.', 'พ.ย.', 'ธ.ค.'];
            return month[num];
        }

        function DayColor() {
            var colors = ['darkred', 'yellow', 'deeppink', 'green', 'orange', 'deepskyblue', 'purple'];
            var d = new Date();
            var color = colors[d.getDay()];
            document.write('<style> .DayColor { background-color: ' + color + '; } </style>');
        }
        //DayColor();

        function fn_dateThai(D) {
            if (D != '') {
                var arD = D.split(' ');
                var newD;
                if (arD.length > 1) {
                    newD = arD[0].split('/');
                }
                else {
                    newD = D.split('/');
                }
                var dateTH = newD[1] + '/' + newD[0] + '/' + newD[2];
                document.write(dateTH);
            }
        }

        function blockKeyCode(event) {
            var key = event.keyCode;
            //alert(key);
            var iChars = "@_!$%^*+=[]\\\';{}|\":<>?";
            var n = iChars.indexOf(String.fromCharCode(key));
            if (n >= 0) {
                event.preventDefault();
                //alert('ห้ามใช้ตัวอักษรพิเศษเหล่านี้ !@#$%^&*()+=[]\\\';,/{}|\":<>? ');
                fn_AlertModal('Warning','ห้ามใช้ตัวอักษรพิเศษเหล่านี้ @_!$%^*+=[]\\\';{}|\":<>?','',0);
            }
        }
    </script>
